<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.justjava">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="DATABASE"
            android:value="justjava.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.example.justjava" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <!--intent filter itu untuk berarti, activity main akan run saat launcher-->
        </activity>
        <activity android:name=".HistoryActivity" />   <!--mendaftarkan historyactivity-->
        <activity android:name=".CalculatorActivity" /> <!--mendaftarkan calculatoractivity-->
        <activity android:name=".BirthdayActivity" /><!--mendaftarkan birthdayactivity-->




    </application>

</manifest>